# QA Environment Configuration
ENVIRONMENT=qa
DEBUG=false
LOG_LEVEL=INFO

# Application Settings
APP_NAME=Collatz Data Science - QA
APP_VERSION=1.0.0
APP_HOST=0.0.0.0
APP_PORT=8080

# Database Configuration
DB_HOST=qa-db-host
DB_PORT=5432
DB_NAME=collatz_db_qa
DB_USER=postgresql
DB_PASSWORD=qa_database_password_here
DB_URL=postgresql://postgresql:qa_database_password_here@qa-db-host:5432/collatz_db_qa

# Jupyter Configuration
JUPYTER_PORT=8888
JUPYTER_TOKEN=qa_jupyter_token
JUPYTER_PASSWORD=qa_jupyter_password

# Data Paths
DATA_RAW_PATH=./data/raw
DATA_PROCESSED_PATH=./data/processed
REPORTS_PATH=./reports
NOTEBOOKS_PATH=./notebooks

# Computation Settings
MAX_WORKERS=4
CACHE_ENABLED=true
BATCH_SIZE=1000
MAX_ITERATIONS=1000000

# API Keys (QA environment)
# OPENAI_API_KEY=qa_openai_api_key_here
# WANDB_API_KEY=qa_wandb_api_key_here
# NEPTUNE_API_TOKEN=qa_neptune_api_token_here

# Security
SECRET_KEY=qa_secret_key_change_in_production
ALLOWED_HOSTS=qa.collatz.example.com,localhost,127.0.0.1

# Docker Configuration
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1

# Python Configuration
PYTHONPATH=/app/src

# QA Specific Settings
TEST_DATA_ENABLED=true
MOCK_EXTERNAL_APIS=true
PERFORMANCE_MONITORING=true